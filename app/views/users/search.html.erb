<div class="common_wrapper">
  <h1>検索結果
  <%= form_with url: search_users_path, method: :get, local: true do |f| %>
    <%= f.text_field :name %>
    <%= f.submit :search %>
  <% end %></h1>
  <table>
    <tbody>
      <% @users.each do |user| %>
        <div>
        <% if user.picture? %>
              <%= image_tag user.picture class:"user-icon_small"%>
              <% else %>
              <%= image_tag "default.jpg", alt: "user-icon", class: "user-icon_small" %>
              <% end %>
        <%= link_to user.name, user %>
        <hr>
        </div>
      <% end %>
    </tbody>
  </table>
  <%= link_to "戻る", users_path %>
</div>
