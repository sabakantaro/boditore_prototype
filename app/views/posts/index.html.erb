<div class="common_wrapper">
  <div id="main">

    <v-form>
        <v-container>
          <v-row class="post__search">
            <%= form_with url: search_posts_path, method: :get, local: true do |f| %>
            <%= f.text_field :title, :placeholder => "検索キーワード" %>
          <%= f.submit :検索 %>
        <% end %>
        </v-row>
    </v-container>
  </v-form>
      
        
  <contents></contents>
  <navbar></navbar>
  </div>
</div>

<%= javascript_pack_tag 'main' %>
